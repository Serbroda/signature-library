    "build:gulp": "gulp build",
    "build:docs": "gulp copyDocs",
    "build": "npm run build:tsc && gulp"
  },
  "keywords": [
    "signature",
    "pad"
  ],
  "author": "Danny Rottstegge",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-copy": "^4.0.1",
    "gulp-minify": "^3.1.0",
    "gulp-watch": "^5.0.1",
    "typescript": "^3.9.5"
  }
}
